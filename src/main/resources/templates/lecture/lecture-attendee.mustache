{{>layout/header}}



<!--여기서 본편-->
<div   style="padding-top: 0px; ">
    <!--여기서 본편 내용-->
    <div class="container justify-content-around">

        <div class="d-flex flex-row mb-3" >
            <div class="p-2 bd-highlight"> {{>layout/sidebar}}</div>

            <div class="d-flex flex-column flex-shrink-1  p-3 flex-fill" >

                <H1> 수강생 목록 </H1>

                <ol class="list-group list-group-numbered">
                    {{#attendee}}
                    <li class="list-group-item d-flex justify-content-between align-items-start">
                        <input type="hidden" id="attendeeId{{id}}" value="{{id}}">
                        <div class="ms-2 me-auto">
                            <img src={{picture}} alt="" width="64" height="64" class="rounded  me-2">
                            {{name}}
                        </div>

                    </li>
                    {{/attendee}}
                    {{^attendee}}
                        <li class="list-group-item d-flex justify-content-between align-items-start">

                            <div class="ms-2 me-auto">
                                수강생이 없습니다.
                            </div>

                        </li>
                    {{/attendee}}

                </ol>


            </div>

        </div>



    </div>
</div>


{{>layout/footer}}